{
  "private": true,
  "author": "Karolis Å arapnickis",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf .cache public",
    "deploy": "gatsby-plugin-s3 deploy -y",
    "dev": "GATSBY_DATA_TARGET=${GATSBY_DATA_TARGET:=esc} gatsby develop",
    "format": "run-p format:*",
    "format:js": "prettier --write '**/*.*'",
    "format:package-json": "sort-package-json",
    "lint": "eslint .",
    "qa": "yarn lint",
    "release": "run-s release:*",
    "release:e": "GATSBY_DATA_TARGET=esc run-s clean build deploy",
    "release:s": "GATSBY_DATA_TARGET=statrem run-s clean build deploy",
    "serve-build": "gatsby serve",
    "try": "rm -rf public && GATSBY_DATA_TARGET=${GATSBY_DATA_TARGET:=esc} run-s build serve-build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,mdx}": [
      "pretty-quick --staged"
    ],
    "package.json": [
      "sort-package-json",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "dot-object": "^1.7.0",
    "font-awesome": "^4.7.0",
    "gatsby": "^5.3.3",
    "gatsby-link": "^5.3.1",
    "gatsby-plugin-google-analytics": "^5.3.0",
    "gatsby-plugin-netlify-cms": "^7.3.0",
    "gatsby-plugin-react-helmet": "^6.3.0",
    "gatsby-plugin-robots-txt": "^1.8.0",
    "gatsby-plugin-sass": "^6.3.1",
    "gatsby-plugin-sharp": "^5.3.2",
    "gatsby-plugin-sitemap": "^6.3.1",
    "gatsby-plugin-styled-components": "^6.3.0",
    "gatsby-source-filesystem": "^5.3.1",
    "gatsby-transformer-remark": "^6.3.2",
    "gatsby-transformer-sharp": "^5.3.1",
    "netlify-cms-app": "^2.9.7",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-burger-menu": "^2.5.2",
    "react-dom": "^18.2.0",
    "react-helmet": "^5.2.0",
    "react-images": "^0.5.17",
    "react-intl": "^2.4.0",
    "react-lazyload": "^2.3.0",
    "remark-parse": "^10.0.1",
    "styled-components": "^5.3.6",
    "tachyons-sass": "^4.9.3"
  },
  "devDependencies": {
    "@k.sh/eslint-config": "1.13.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-styled-components": "^2.0.7",
    "cz-conventional-changelog": "^3.0.2",
    "gatsby-plugin-advanced-sitemap": "^2.1.0",
    "gatsby-plugin-layout": "^4.3.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-s3": "^0.3.8",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "sass": "^1.57.1",
    "serve": "^10.1.2",
    "sort-package-json": "^1.22.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
