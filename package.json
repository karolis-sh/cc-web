{
  "name": "cc-web",
  "version": "0.0.0",
  "author": "Karolis <pastas.k@gmail.com>",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "GATSBY_DATA_TARGET=${GATSBY_DATA_TARGET:=esc} gatsby develop",
    "serve-build": "gatsby serve",
    "try-build": "rm -rf public && GATSBY_DATA_TARGET=${GATSBY_DATA_TARGET:=esc} run-s build serve-build",
    "clean": "rm -rf .cache public",
    "lint": "eslint .",
    "format": "prettier '**/*.*' --write",
    "qa": "yarn lint",
    "precommit": "lint-staged",
    "prepush": "yarn qa"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "dot-object": "^1.7.0",
    "font-awesome": "^4.7.0",
    "gatsby": "^1.9.273",
    "gatsby-link": "^1.6.45",
    "gatsby-plugin-google-analytics": "^1.0.31",
    "gatsby-plugin-netlify-cms": "^2.0.1",
    "gatsby-plugin-offline": "^1.0.18",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "gatsby-plugin-robots-txt": "^1.1.0",
    "gatsby-plugin-sass": "^1.0.26",
    "gatsby-plugin-sharp": "^1.6.48",
    "gatsby-plugin-sitemap": "^1.2.25",
    "gatsby-plugin-styled-components": "^2.0.11",
    "gatsby-source-filesystem": "^1.5.39",
    "gatsby-transformer-remark": "^1.7.44",
    "gatsby-transformer-sharp": "^1.6.27",
    "netlify-cms": "^2.0.1",
    "react-burger-menu": "^2.5.2",
    "react-helmet": "^5.2.0",
    "react-images": "^0.5.17",
    "react-intl": "^2.4.0",
    "react-lazyload": "^2.3.0",
    "remark-parse": "^5.0.0",
    "styled-components": "^3.3.3",
    "tachyons-sass": "^4.9.3"
  },
  "devDependencies": {
    "@k.sh/eslint-config": "1.1.0",
    "babel-eslint": "^8.2.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.1.0",
    "eslint-config-airbnb": "17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.9.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.7",
    "serve": "^9.2.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
